{
  "mode": "local",
  "vectorStore": {
    "backend": "milvus",
    "config": {
      "address": "localhost:19530"
    }
  },
  "embedding": {
    "provider": "transformers",
    "model": "Xenova/all-MiniLM-L6-v2",
    "local": true
  },
  "indexing": {
    "languages": ["typescript", "javascript", "python"],
    "excludePatterns": [
      "node_modules/**",
      "dist/**",
      ".git/**"
    ],
    "includeDocuments": true
  }
}
